{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "unused-imports", "@stylistic/js"],
  "rules": {
    "unused-imports/no-unused-imports-ts": 2,
    "@stylistic/js/indent": ["error", 2]
  }
}
